dna_base_comp <- function(base) {
if (base == "A") {
return("T")
} else if (base == "C") {
return("G")
} else if (base == "G") {
return("C")
} else if (base == "T") {
return("A")
}
return(NA)
}
my_base <- "C"
dna_base_comp(my_base)
dna_base_to_rna(my_base)
dna_base_to_rna <- function(base){
bases <- c("A","G","C","U")
names(bases) <- c("U","C","G","A")
return(bases[base])
}
my_base <- "A"
dna_base_comp(my_base)
dna_base_to_rna(my_base)
dna_base_to_rna(my_base)
dna_base_comp <- function(seq) {
seqVec <- strsplit(seq,"")
bases <- c("A","G","C","T")
names(bases) <- c("T","G","C","A")
seqCompVec <- seqVec
for (i in length(seqVec)) {
seqCompVec[i] <-bases[seqVec[i]]
}
seqComp <- paste(seqCompVec,collapse = "")
return(seqComp)
}
my_base <- "ATGC"
dna_base_comp(my_base)
